{
    "extends": "eslint-config-fluid",
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "key-spacing": ["Error"]
    }
}
